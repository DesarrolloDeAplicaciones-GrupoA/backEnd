<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field"
                   default-cascade="save-update">
    <class name="ar.edu.unq.desapp.grupoA.models.Travel">
        <id column="ID" name="id">
            <generator class="native" />
        </id>
        <property name="nameTravel" />
        <property name="fuelCost" type="java.lang.Integer" />
        <property name="tollCost" type="java.lang.Integer"/>
        <property name="rangeHours" type="org.joda.time.Interval"/>

    </class>
</hibernate-mapping>